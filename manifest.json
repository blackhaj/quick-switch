{
  "name": "Quick Switch",
  "description" : "An extension to quick switch between your current webpage and a more family friendly friendly version",
  "version": "1.0",
  "permissions": ["tabs", "commands", "background"],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.1.1.min.js","cat.js"]
  }],
  "page_action": {
    "default_icon": {
      "16": "wink.png",
      "32": "wink.png",
      "48": "wink.png",
      "128": "wink.png"
    }
  }
}